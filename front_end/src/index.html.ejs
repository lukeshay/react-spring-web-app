      crossorigin="anonymous"
    ></script>
    <% } else { %>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/react/16.10.2/umd/react.development.js"
      integrity="sha256-Fy0+oh76u+dih9imNzr0Ka1RHbVKV8o4nR+p8s6wTXE="

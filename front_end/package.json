{
  "name": "front_end",
  "version": "1.1.1",
  "private": true,
  "homepage": "http://lukeshay.com",
  "main": "src/index.tsx",
  "author": "Luke Shay",
  "license": "LGPL",
  "scripts": {
    "start": "webpack-dev-server --open --hot --config configs/webpack/dev.js",
    "start:local": "webpack-dev-server --open --hot --config configs/webpack/local.js",
    "prebuild": "yarn",
    "build": "webpack -p --config=configs/webpack/prod.js",
    "build:analyze": "yarn build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "deploy": "pm2 serve dist --name web-app -- 3000",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx}'",
    "format": "prettier --write \"**/*.+(ts|tsx|json|css|md)\"",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "validate": "yarn typecheck && yarn lint",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@types/material-ui": "^0.21.7",
    "@types/node": "^13.1.1",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "eslint-plugin-react": "^7.18.0",
    "material-ui": "^1.0.0-beta.47",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-inlinesvg": "^1.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "react-toastify": "^5.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.8.3",
    "@loadable/component": "^5.11.0",
    "@svgr/webpack": "^5.0.1",
    "@types/flux": "^3.1.9",
    "@types/jest": "^25.1.1",
    "@types/universal-cookie": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "brotli-webpack-plugin": "^1.1.0",
    "clean-webpack-plugin": "*",
    "css-loader": "^3.4.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jest-localstorage-mock": "^2.4.0",
    "node": "^13.3.0",
    "node-sass": "^4.13.0",
    "pm2": "^4.2.3",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.18",
    "react-test-renderer": "^16.12.0",
    "react-transition-group": "^4.3.0",
    "sass-loader": "^8.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.2",
    "ts-jest": "^25.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "typescript-tslint-plugin": "^0.5.5",
    "universal-cookie": "^4.0.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "jest-junit": {
    "output": "./reports/jest-junit.xml",
    "ancestorSeparator": " , ",
    "classNameTemplate": ""
  }
}

{
  "presets": [
    ["@babel/preset-env", { "modules": false }],
    "@babel/preset-react"
  ],
  "plugins": ["react-hot-loader/babel"],
  "env": {
    "production": {
      "presets": ["minify"]
    },
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": ["babel-plugin-dynamic-import-node"]
    }
  }
}
